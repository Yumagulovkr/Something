class Arrow:
    def __init__(self, x, y, damage, poison=False):
        self.x = x
        self.y = y
        self.damage = damage
        self.poison = poison
    
    def fly(self, direction_x, direction_y):
        # –ª–æ–≥–∏–∫–∞ –ø–æ–ª–µ—Ç–∞ —Å—Ç—Ä–µ–ª—ã –≤ –∑–∞–¥–∞–Ω–Ω–æ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–∏
        pass
    
    def hit(self, target):
        target.receive_damage(self.damage)
        if self.poison:
            target.poisoned = True

class GuidedArrow(Arrow):
    def fly_to_target(self, target):
        # –ª–æ–≥–∏–∫–∞ –ø–æ–ª–µ—Ç–∞ —Å—Ç—Ä–µ–ª—ã –∫ —Ü–µ–ª–∏, –±–µ–∑ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏ –ø—Ä–∏—Ü–µ–ª–∏–≤–∞–Ω–∏—è
        pass

class PoisonArrow(Arrow):
    def __init__(self, x, y, damage):
        super().__init__(x, y, damage, poison=True)


–ó–¥–µ—Å—å —Å–æ–∑–¥–∞–µ—Ç—Å—è –±–∞–∑–æ–≤—ã–π –∫–ª–∞—Å—Å Arrow, –∫–æ—Ç–æ—Ä—ã–π –∏–º–µ–µ—Ç –æ–±—â–∏–µ —Å–≤–æ–π—Å—Ç–≤–∞ —Ç–∞–∫–∏–µ –∫–∞–∫ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã, —É—Ä–æ–Ω –∏ –ø—Ä–∏–∑–Ω–∞–∫ –æ—Ç—Ä–∞–≤–ª–µ–Ω–∏—è. –ú–µ—Ç–æ–¥ fly() –æ–ø–∏—Å—ã–≤–∞–µ—Ç –ª–æ–≥–∏–∫—É –ø–æ–ª–µ—Ç–∞ —Å—Ç—Ä–µ–ª—ã –≤ –∑–∞–¥–∞–Ω–Ω–æ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–∏, –∞ –º–µ—Ç–æ–¥ hit() –æ–ø–∏—Å—ã–≤–∞–µ—Ç –ª–æ–≥–∏–∫—É –ø–æ–ø–∞–¥–∞–Ω–∏—è —Å—Ç—Ä–µ–ª—ã –≤ —Ü–µ–ª—å.

